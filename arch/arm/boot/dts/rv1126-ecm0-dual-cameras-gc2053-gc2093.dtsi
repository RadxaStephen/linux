/ {
	compatible = "edgeble,edge-compute-module-0-carrier",
		     "edgeble,edge-compute-module-0", "rockchip,rv1126";

	avdd_2v8: avdd-2v8-regulator {
		compatible = "regulator-fixed";
		regulator-name = "avdd_2v8";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		vin-supply = <&vcc_3v3_sys>;
	};

	dovdd_1v8: dovdd-1v8-regulator {
		compatible = "regulator-fixed";
		regulator-name = "dovdd_1v8";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&vcc_3v3_sys>;
	};

	dvdd_1v2: dvdd-1v2-regulator {
		compatible = "regulator-fixed";
		regulator-name = "dvdd_1v2";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1200000>;
		vin-supply = <&vcc_3v3_sys>;
	};
};

// --- J15 start

&i2c1 {
	status = "okay";
	clock-frequency = <400000>;

	gc2093: gc2093@3f {
		status = "okay";
		compatible = "galaxycore,gc2093";
		reg = <0x3f>;

		clocks = <&cru CLK_MIPICSI_OUT>;
		clock-names = "xvclk";

		power-domains = <&power RV1126_PD_VI>;
		pinctrl-names = "rockchip,camera_default";
		pinctrl-0 = <&mipicsi_clk0>;
		pwdn-gpios = <&gpio1 RK_PD4 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio1 RK_PD6 GPIO_ACTIVE_LOW>;

		avdd-supply = <&avdd_2v8>;
		dovdd-supply = <&dovdd_1v8>;
		dvdd-supply = <&dvdd_1v2>;

		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "front";
		rockchip,camera-module-name = "YT-RV1109-2-V1";
		rockchip,camera-module-lens-name = "40IR-2MP-F20";

		// port {
		// 	ucam_out0: endpoint {
		// 		remote-endpoint = <&mipi_in_ucam0>;
		// 		data-lanes = <1 2>;
		// 	};
		// };
	};
};

// --- J15 end

// --- j16 start

&i2c1 {
	status = "okay";
	clock-frequency = <400000>;

	gc2053: gc2053@37 {
		status = "okay";
		compatible = "galaxycore,gc2053";
		reg = <0x37>;

		clocks = <&cru CLK_MIPICSI_OUT>;
		clock-names = "xvclk";

		power-domains = <&power RV1126_PD_VI>;
		pinctrl-names = "rockchip,camera_default";
		pinctrl-0 = <&mipicsi_clk1>;
		pwdn-gpios = <&gpio1 RK_PD5 GPIO_ACTIVE_LOW>;
		reset-gpios = <&gpio1 RK_PD7 GPIO_ACTIVE_LOW>;

		avdd-supply = <&avdd_2v8>;
		dovdd-supply = <&dovdd_1v8>;
		dvdd-supply = <&dvdd_1v2>;

		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "front";
		rockchip,camera-module-name = "YT-RV1109-2-V1";
		rockchip,camera-module-lens-name = "40IR-2MP-F20";

		port {
			ucam_out0: endpoint {
				remote-endpoint = <&mipi_in_ucam0>;
				data-lanes = <1 2>;
			};
		};
	};
};

&csi_dphy1 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_ucam0: endpoint@1 {
				remote-endpoint = <&ucam_out0>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			csi_dphy1_out: endpoint@0 {
				remote-endpoint = <&mipi_csi2_input>;
			};
		};
	};
};

&mipi_csi2 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_csi2_input: endpoint@1 {
				remote-endpoint = <&csi_dphy1_out>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_csi2_output: endpoint@0 {
				remote-endpoint = <&cif_mipi_in>;
				data-lanes = <1 2>;
			};
		};
	};
};

&rkcif_mipi_lvds {
	status = "okay";

	port {
		cif_mipi_in: endpoint {
			remote-endpoint = <&mipi_csi2_output>;
			data-lanes = <1 2>;
		};
	};
};

&rkcif_mipi_lvds_sditf {
	status = "okay";

	port {
		mipi_lvds_sditf: endpoint {
			remote-endpoint = <&isp_vir0_in>;
			data-lanes = <1 2>;
		};
	};
};

/* isp cma buffer don't fiddle with it, dual camera(1920 * 1080) need 92M buffer
&isp_reserved {
	size = <0x5c00000>;
};
 */

&rkisp {
	status = "okay";
};

&rkisp_mmu {
	status = "disabled";
};

&rkisp_vir0 {
	status = "okay";

	ports {
		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			isp_vir0_in: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&mipi_lvds_sditf>;
			};
		};
	};
};

&rkispp {
	status = "okay";
	/* the max input w h and fps of mulit sensor */
	max-input = <1920 1080 30>;
};

&rkispp_mmu {
	status = "okay";
};

&rkispp_vir0 {
	status = "okay";
};

// --- J16 end
